pipeline {
    agent any
    stages {
        stage('Git clone') 
           steps {
           echo "hi"
        }
        
        stage('Verify') 
            steps {
            echo "hello"
        }
        
        stage('Build') 
            steps {
            echo "hi there"
        }


        stage('Run Test') 
            steps {
            echo "this is freda"
         }
        
         stage('Run mvn commands') 
            steps {
            echo "this is a test only"
            }
        }
    }
