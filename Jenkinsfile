pipeline {
    agent any
    stages {
        stage('Git clone') {
           echo 'hi'
        }
        
        stage('Verify') {
            echo 'hello'
        }
        
        stage('Build') {
            echo 'hi there'
        }


        stage('Run Test') {
            echo 'this is freda'
         }
        
         stage('Run mvn commands') {
            echo 'this is a test'
            }
        }
    }
